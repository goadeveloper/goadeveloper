{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
    "exclude": [
        "dist"
    ],
    "compilerOptions": {
        "types": [
            "./worker-configuration.d.ts",
            "node"
        ],
        "baseUrl": ".",
        "paths": {
            "@components/*": ["src/components/*"],
            "@assets/*": ["src/assets/*"],
            "@lib/*": ["src/lib/*"],
        }
    }
}
